{
  "name": "@cross-platform/example",
  "version": "1.0.0",
  "description": "跨端框架示例应用",
  "main": "src/index.js",
  "scripts": {
    "dev": "node --no-warnings ../cli/dist/index.js dev",
    "dev:h5": "node --no-warnings ../cli/dist/index.js dev -p h5",
    "dev:rn": "node --no-warnings ../cli/dist/index.js dev -p rn",
    "dev:weapp": "node --no-warnings ../cli/dist/index.js dev -p weapp",
    "build": "node --no-warnings ../cli/dist/index.js build",
    "build:h5": "node --no-warnings ../cli/dist/index.js build -p h5",
    "build:rn": "node --no-warnings ../cli/dist/index.js build -p rn",
    "build:weapp": "node --no-warnings ../cli/dist/index.js build -p weapp",
    "build:all": "node --no-warnings ../cli/dist/index.js build -p all",
    "test": "vitest"
  },
  "keywords": [
    "cross-platform",
    "example",
    "demo"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@cross-platform/components": "workspace:*",
    "@cross-platform/core": "workspace:*",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@cross-platform/cli": "workspace:*",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}